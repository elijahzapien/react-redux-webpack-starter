{
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "globals": {},
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "classes": true,
      "experimentalObjectRestSpread": true,
      "jsx": true
    }
  },
  "plugins": [
    "babel",
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "standard-react"
  ],
  "rules": {
    "indent": [
      1,
      2,
      { "SwitchCase": 1 }
    ],
    "no-unused-vars": [
      1,
      {
        "vars": "all",
        "args": "none",
        "varsIgnorePattern": "React"
      }
    ],
    "quotes": [
      1,
      "single"
    ],
    "linebreak-style": [
      1,
      "unix"
    ],
    "semi": [
      1,
      "always"
    ],
    "no-console": 0
  }
}